{
    "type": "BreathingWindow",
    "skin": "HUD",
    "contents": {
        "type": "relativeLayout",
        "contents": [
            {
                "type" : "UIIconBar",
                "id" : "breathBar",
                "icon" : "engine:icons.bubble",
                "family" : "breathBar",
                "emptyIcon" : "engine:icons.burstBubble",
                "halfIconMode" : "shrink",
                "spacing" : 2,
                "maxIcons" : 10,

                "layoutInfo" : {
                                    "use-content-width" : true,
                                    "use-content-height" : true,
                                    "position-left" : {
                                        "target" : "CENTER"
                                    },
                                    "position-right" : {
                                        "target" : "RIGHT",
                                        "widget" : "toolbar"
                                    },
                                    "position-bottom" : {
                                        "target" : "TOP",
                                        "widget" : "toolbar",
                                        "offset" : 1
                                    }
                }


            }
        ]
    }
}
